<section class="icon-list stars">

  <i class="nes-icon is-medium star"></i>
  <i class="nes-icon is-medium star is-transparent"></i>
  <i class="nes-icon is-medium star is-transparent"></i>
  <i class="nes-icon is-medium star is-transparent"></i>
  <i class="nes-icon is-medium star is-transparent"></i>

</section>

<style>
  .icon-list {
    transform-origin: center;
    transform: scale(.8);
  }

  .stars {
    animation: appearStars both;
    view-timeline-name: --chillCat;
    view-timeline-axis: block;
    animation-timeline: --chillCat;
    animation-range: entry 5% cover 30%;
  }

  @keyframes appearStars {
    from {
      opacity: 0;
      scale: 0;
    }
    to {
      opacity: 1;
      scale: 1;
    }
  }
</style>
