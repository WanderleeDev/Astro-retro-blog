{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [ "src/components/*" ],
      "@sharedComponents/*": [ "src/shared/*" ],
      "@data/*": [ "src/data/*" ],
      "@interfaces/*": [ "src/interfaces/*" ],
      "@layouts/*": [ "src/layouts/*" ],
      "@pages/*":["src/pages/*"],
      "@scripts/*": [ "src/scripts/*" ]
    }
  }
}
